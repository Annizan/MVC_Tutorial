@using HelloWorld.Models

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ListeClients</title>
</head>
<body>
<table>
    <tr>
        <th>Nom</th>
        <th>Age</th>
    </tr>
    @foreach (var client in ViewData["Clients"] as List<Client>)
    {
        <tr>
            <td>@client.Nom</td>
            <td>@client.Age</td>
        </tr>

    }

</table>

</body>
</html>
